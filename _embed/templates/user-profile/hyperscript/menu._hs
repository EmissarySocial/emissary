behavior menu(selected)

	on load
		tell <.menu-option /> in me
			if your [@data-filterId] is selected then 
				add .selected to you
			else
				remove .selected from you
			end
		end
	end

	on click (target) from <.menu-option/> in me
		send closeModal
		set targetUrl to the target's [@href]
		go to url `${targetUrl}`
	end
end