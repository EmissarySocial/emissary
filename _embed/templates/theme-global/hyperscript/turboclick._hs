on __mousedown or __touchstart

	log event
	if (event.button is 1) or (event.buttons is 1) then
	else
		log "exit"
		exit
	end

	log "continue"

	set node to the closest <.turboclick /> to the event.target

	if node is not empty then
		halt the event
		send click(turboclick:true) to node
	end

end

on __click[detail.turboclick == undefined]

	log event
	set node to the closest <.turboclick /> to the event.target
	log node
	if node is not empty then
		halt the event
		exit
	end
	log "continue?"
end
