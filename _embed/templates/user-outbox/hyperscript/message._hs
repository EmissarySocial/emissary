behavior message(folderId, readDate)

	init
		fetch `/@me/pub/folder/readDate?folderId=${folderId}&readDate=${readDate}` with method:"POST"
	end

	on keydown[key=="ArrowLeft"] from window
		halt the event
		send click to #previous in me
	end
	
	on keydown[key=="ArrowRight"] from window
		halt the event
		send click to #next in me
	end
end