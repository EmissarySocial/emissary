{
	"label": "RSS Article",
	"description": "Link to an external article from an RSS Feed.",
	"containedBy": [
		"rss-folder"
	],
	"schema": {
		"type": "object",
		"properties": {}
	},
	"roles": {
		"owner": {
			"label": "Domain Owner",
			"decription": "Full control"
		}
	},
	"states": {
		"unread": {
			"label": "Unread"
		}, 
		"read": {
			"label": "Read"
		}
	},
	"actions": {
		"view": {
			"method": "view-stream"
		},
		"mark-read": {
			"method": "update-state",
			"states": ["unread"],
			"newStateId": "read"
		},
		"mark-unread": {
			"method": "update-state",
			"states": ["read"],
			"newStateId": "unread"
		}
	}
}